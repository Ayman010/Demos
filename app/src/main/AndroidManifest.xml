<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.android.demos">

    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <uses-feature android:name="android.hardware.camera"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Shared_Preferences"/>
        <activity android:name=".ChangeTextSizeBySeekBar"/>
        <activity android:name=".Calculator"/>
        <activity android:name=".OpenFile"/>
        <activity android:name=".UploadFromGallary"/>
        <activity android:name=".Zoom"/>
        <activity android:name=".Camera"/>
        <activity android:name=".NextPage"/>
        <activity android:name=".Bluetooth"/>
        <activity android:name=".InternalStorage">
        </activity>
        <activity android:name=".DBActivity">
        </activity>
        <activity android:name=".CacheIE">
        </activity>
        <activity android:name=".SqliteDataBase">
        </activity>
    </application>

</manifest>